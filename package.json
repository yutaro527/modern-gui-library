{
  "name": "@yutaro527/modern-gui-library",
  "version": "1.0.0",
  "description": "A modern GUI library with reusable components like slideshows, buttons, and more",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run lint && npm run typecheck",
    "prepare": "npm run build"
  },
  "keywords": [
    "ui",
    "components",
    "react",
    "typescript",
    "modern",
    "gui"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@ladle/react": "^5.0.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "typescript": "^5.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "dist",
    "README.md",
    "src/theme/themes.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yutaro527/modern-gui-library.git"
  },
  "homepage": "https://yutaro527.github.io/modern-gui-library/",
  "publishConfig": {
    "access": "public"
  }
}
